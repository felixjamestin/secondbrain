{
  "expo": {
    "name": "Ramana Maharishi Daily",
    "description": "App that surfaces insights from Ramana Maharishi",
    "icon": "./assets/rmed-prod-icon.png",
    "version": "1.0.0",
    "slug": "ramanamaharishidaily",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "ios": {
      "bundleIdentifier": "com.felixjamestin.ramanamaharishidaily",
      "supportsTablet": true
    },
    "android": {
      "package": "com.felixjamestin.ramanamaharishidaily"
    },
    "orientation": "portrait",
    "splash": {
      "image": "./assets/rmed-prod-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#222222"
    },
    "scheme": "ramanamaharishidaily",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "assetBundlePatterns": ["**/*"],
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#222222"
    }
  }
}
